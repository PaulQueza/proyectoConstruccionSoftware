n<script>\nimport { Icon } from \"@iconify/vue2\";\nexport default {\n    data: () => ({\n        name: null,\n        descripcion: null,\n        precio: null,\n        imagen: null,\n        drawer: false,\n        zapatillas: [\n            {\n                id: 1,\n                nombre: 'Jordan 1',\n                marca: 'Nike',\n                talla: '12 US',\n                imagen: require(\"../assets/zapatillaDefault.png\"),\n                precio: '125.000',\n                descripcion: 'Ta wena la jordan oe',\n            },\n            {\n                id: 2,\n                nombre: 'Jordan 2',\n                talla: '11 US',\n                marca: 'Adidas',\n                imagen: require(\"../assets/zapatillaDefault.png\"),\n                precio: '200.000',\n                descripcion: 'Si el 1 es weno, el 2 es mejor',\n            },\n            {\n                id: 3,\n                nombre: 'Jordan 3',\n                talla: '23 EUR',\n                marca: 'Adidas',\n                imagen: require(\"../assets/zapatillaDefault.png\"),\n                precio: '250.000',\n                descripcion: 'Es una breve descripcion de la zapatilla 3',\n            },\n            {\n                id: 4,\n                nombre: 'Jordan 4',\n                talla: '24 EUR',\n                marca: 'Lol',\n                imagen: require(\"../assets/zapatillaDefault.png\"),\n                precio: '1.000.000',\n                descripcion: 'Descripcion zapatilla 4',\n            },\n        ],\n        rules: [\n            value => !value || value.size < 2000000 || '¡La imagen no puede pesar mas de 2MB!',\n        ],\n    }),\n    components: {\n        Icon,\n    },\n    methods: {\n        agregarProducto() {\n            if (this.name === '' || this.descripcion === '' || this.precio === '' || this.imagen === '') {\n                console.log(\"Datos vacios\")\n            } else {\n                this.zapatillas.push({\n                    id: Date.now(),\n                    nombre: this.name,\n                    descripcion: this.descripcion,\n                    precio: this.precio,\n                })\n                this.name = null\n                this.descripcion = null\n                this.precio = null\n            }\n        },\n        eliminarProducto(id) {\n            console.log(id)\n            this.zapatillas = this.zapatillas.filter(e => e.id != id)\n        },\n        algo(){\n            console.log(\"wena xdxdddxdd\")\n        }\n    }\n}\n</script>"]},"metadata":{},"sourceType":"module"}