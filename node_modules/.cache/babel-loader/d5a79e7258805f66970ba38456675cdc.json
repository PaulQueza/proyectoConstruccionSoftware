s.image = null\n            }\n        },\n        eliminarProducto(id) {\n            console.log(id)\n            this.zapatillas = this.zapatillas.filter(e => e.id != id)\n        }\n    }\n}\n</script>"]},"metadata":{},"sourceType":"module"}